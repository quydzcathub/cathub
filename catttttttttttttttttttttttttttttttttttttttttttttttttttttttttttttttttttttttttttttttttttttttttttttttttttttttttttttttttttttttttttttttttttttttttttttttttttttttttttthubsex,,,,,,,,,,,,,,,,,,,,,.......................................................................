                SP = "Default"
            end
        elseif SelectMaterial == "Scrap Metal" then
            if First_Sea then
                MMon = "Brute"
                MPos = CFrame.new(-1145, 15, 4350)
                SP = "Default"
            elseif Second_Sea then
